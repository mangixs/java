<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.classes.style.mapper.ClassMessageMapper">
    <select id="getMessageList" resultType="com.classes.style.entity.ClassMessage">
        select id,username,updated_at,office,context from class_message where is_valid = 1
    </select>
</mapper>